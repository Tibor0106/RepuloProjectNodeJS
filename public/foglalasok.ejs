<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>EuroJET | Foglalások</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
</head>

<body>
    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Teljes név</th>
                    <th scope="col">Email</th>
                    <th scope="col">Telefon</th>
                    <th scope="col">Honnan</th>
                    <th scope="col">Hova</th>
                </tr>
            </thead>
            <tbody>
                <% for (var foglalas of _foglalasok) { %>
                    <% for (var destination of _des) { %>
                        <% if (foglalas.originId===destination.destinationId) { %>
                            <tr>
                                <th scope="row">
                                    <%=foglalas.id%>
                                </th>
                                <td>
                                    <%=foglalas.fullName%>
                                </td>
                                <td>
                                    <%=foglalas.email%>
                                </td>
                                <td>
                                    <%=foglalas.phone%>
                                </td>
                                <td>
                                    <%=destination.destinationName%>
                                </td>
                                <td>
                                    <%=foglalas.destinationName%>
                                </td>

                            </tr>



                            <% } %>
                                <% } %>

                                    <% } %>



            </tbody>
        </table>

    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>
</body>

</html>